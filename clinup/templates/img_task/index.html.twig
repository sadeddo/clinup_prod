{% extends 'base.html.twig' %}

{% block title %}ImgTask index{% endblock %}

{% block body %}
    <h1>ImgTask index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>FilePath</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for img_task in img_tasks %}
            <tr>
                <td>{{ img_task.id }}</td>
                <td>{{ img_task.filePath }}</td>
                <td>
                    <a href="{{ path('app_img_task_show', {'id': img_task.id}) }}">show</a>
                    <a href="{{ path('app_img_task_edit', {'id': img_task.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_img_task_new') }}">Create new</a>
{% endblock %}
